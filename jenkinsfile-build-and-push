node {
  stage('========== Clone repository ==========') {
    checkout scm
  }
  stage('========== Build image ==========') {
    app = docker.build("haedoang/cicd_study")
  }
  stage('========== Push image ==========') {
    docker.withRegistry('https://registry.hub.docker.com', 'haedoang') {
      app.push("${env.BUILD_NUMBER}")
      app.push("latest")
    }
  }
}